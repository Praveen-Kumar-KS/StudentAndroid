<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#feb209"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:text="Result"
        android:layout_marginBottom="100dp"
            android:layout_centerInParent="true"
            android:textColor="#030303"
            android:textSize="30sp" />

    <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginLeft="15.7dp"
      android:layout_marginTop="70dp"
      android:layout_marginRight="15.7dp"
      android:orientation="vertical">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:orientation="vertical">

      <ImageView
          android:layout_width="wrap_content"
          android:layout_height="300dp"
          android:background="@drawable/white_cueve_bg" />
          
          <LinearLayout
          android:id="@+id/linearAdmin"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:orientation="vertical"
                android:layout_marginTop="30dp"
          android:paddingLeft="20.7dp">
          
          <TableLayout
        android:id="@+id/tablelayout"
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:paddingRight="2dip" >

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:layout_weight="1"
                            android:text="Subject 1:"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:id="@+id/subject1Result"
                            android:layout_weight="1"
                            android:text="No Data Found"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:layout_weight="1"
                            android:text="Subject 2:"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:id="@+id/subject2Result"
                            android:layout_weight="1"
                            android:text="No Data Found"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                    </TableRow>
                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:layout_weight="1"
                            android:text="Subject 3:"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:id="@+id/subject3Result"
                            android:layout_weight="1"
                            android:text="No Data Found"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                    </TableRow>
                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:layout_weight="1"
                            android:text="Subject 4:"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:id="@+id/subject4Result"
                            android:layout_weight="1"
                            android:text="No Data Found"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:layout_weight="1"
                            android:text="Subject 5:"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:id="@+id/subject5Result"
                            android:layout_weight="1"
                            android:text="No Data Found"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:layout_weight="1"
                            android:text="Total:"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <TextView
                            android:fontFamily="@font/poppins_semibold"
                            android:id="@+id/totalResult"
                            android:layout_weight="1"
                            android:text="No Data Found"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                    </TableRow>
        
        </TableLayout>
        
          </LinearLayout>

          </FrameLayout>
          </LinearLayout>
    
    </RelativeLayout>